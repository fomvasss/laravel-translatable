{
    "name": "fomvasss/laravel-translatable",
    "description": "A package to manage SEO (meta-tags, xml-fields, etc.)",
    "type": "composer-package",
    "keywords": ["laravel", "language", "localization", "translation", "laravel-translatable", "multilingual"],
    "authors": [
        {
            "name": "Vasyl Fomin",
            "email": "fomvasss@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Fomvasss\\LaravelTranslatable\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fomvasss\\LaravelTranslatable\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
